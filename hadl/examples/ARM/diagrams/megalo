
model ARM:Dot:Ecore
apply HADLEcore2DotEcore {
   IN=ARM:HADL:Ecore,
   OUT=ARM:Dot:Ecore
}

model ARM:Dot:EBNF
apply DotEcore2DotEBNF {
   IN=ARM:Dot:Ecore,
   OUT=ARM:Dot:EBNF
}

model ARM:PNG:Binary
apply Dot2PNG {
   IN=ARM:Dot:EBNF,
   OUT=ARM:PNG:Binary,
   RANKDIR="TB",
   LABELDISTANCE="3"
}


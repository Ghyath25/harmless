
domain P2A:EBNF filter "p2a.config" abstract

transformation HADLiEcore2P2AEcore (in IN:HADLi:Ecore, in P:PRM:Ecore, out OUT:P2A:Ecore) using ATLEngine:BASH:EBNF {
   TRANSFORMATION_MODEL=HADLi2P2A:ASM:XML,
   SOURCE_MODELS=IN:HADLi:Ecore P:PRM:Ecore,
   TARGET_MODELS=OUT:P2A:Ecore,
   LIBRARY_MODELS=PRMHelpers:ASM:XML
}

transformation P2AEcore2P2AEBNF (in IN:P2A:Ecore, out OUT:P2A:EBNF) using EBNFExtractor:BASH:EBNF {
   SYNTAX_MODEL=P2A:TCS:Ecore,
   SOURCE_MODEL=IN:P2A:Ecore,
   TARGET_MODEL=OUT:P2A:EBNF
}


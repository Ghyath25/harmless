
model P2A:KM3:Ecore
apply KM3EBNF2KM3Ecore {
   IN=P2A:KM3:EBNF,
   OUT=P2A:KM3:Ecore
}

domain P2A:Ecore filter "p2amodel"
apply KM3Ecore2Ecore {
   IN=P2A:KM3:Ecore,
   OUT=P2A:Ecore
}


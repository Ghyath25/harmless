#!/bin/bash

echo "Basicmath small"
echo $'\n'

# Execute qemu
echo "Qemu simulation ..."
time qemu-system-arm -M versatilepb -m 128M -nographic -kernel sources/basicmath_small_modifie.bin | ../qemu/perf_qemu

# Execute harmless
echo $'\n'
echo 'Harmless simulation ...'
time ../../../arm_opt/arm_opt sources/basicmath_small_modifie.elf 1000000000 65544

echo $'\n'
echo "Basicmath large"
echo $'\n'

# Execute qemu
echo "Qemu simulation ..."
time qemu-system-arm -M versatilepb -m 128M -nographic -kernel sources/basicmath_large_modifie.bin | ../qemu/perf_qemu

# Execute harmless
echo $'\n'
echo 'Harmless simulation ...'
time ../../../arm_opt/arm_opt sources/basicmath_large_modifie.elf 2000000000 65544

echo $'\n'

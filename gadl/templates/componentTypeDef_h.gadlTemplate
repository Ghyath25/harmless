%template headerC%
#ifndef __%!MODELNAME%_COMP_TYPEDEF_H__
#define __%!MODELNAME%_COMP_TYPEDEF_H__

%foreach component in componentList do
  foreach typedef in component::typedefList do
  %typedef struct {\n%
  foreach member in typedef::memberList do
    %\t%!member::declaration%;\n%
  end foreach
  %} gadl_type_%!component::name%_%!typedef::name%;\n\n%
  end foreach
end foreach
%#endif
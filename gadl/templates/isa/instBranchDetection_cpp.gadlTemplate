%template headerC%

#include "%!core::NAME%_core.h"
#include "%!core::NAME%_instruction.h"

%if exists core::branchInstList then
  foreach inst in core::branchInstList do
    if not inst::unknown & inst::unconditionnal then
    %void %!inst::name%::detectBranch(%!core::NAME%_core *_core){\n%
    !inst::code
    %}\n\n%
    end if
  end foreach
end if


gui cocoa {

with option gadl_options

with lexique gadl_lexique {
  fileExtension: "cpu"
  fileExtension: "isa"
  fileExtension: "core"
  title: "Core Source"
  blockComment : "--"

  label $core$, $idf$
  label $machine$, $idf$
  label $pipeline$, $idf$
  label $architecture$, $idf$
}


with lexique template_scanner {
  fileExtension : "gadlTemplate"
  title: "Template"
  blockComment : "#"
}

}
